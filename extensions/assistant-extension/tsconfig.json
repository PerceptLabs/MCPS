{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "module": "esnext",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declarationDir": "dist/types",
    "outDir": "dist",
    "importHelpers": true,
    "typeRoots": ["node_modules/@types"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@unroute/sdk/*": ["./node_modules/@unroute/sdk/dist/*"]
    }
  },
  "include": ["src"],
  "exclude": ["rolldown.config.js"]
}
