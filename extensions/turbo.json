{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:publish": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "**.tgz"
      ]
    },
    "dev": {
      "cache": false
    },
    "type-check": {}
  }
}
